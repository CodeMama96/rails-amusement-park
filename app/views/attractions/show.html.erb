<%= link_to "See attractions", "/attractions/edit"%>

<%= link_to "Show #{@attraction.name}", attraction_path %>

<%= form_for @ride do |f| %>
    <%=f.hidden_field :user_id, value: session[:user_id]%>
    <%=f.hidden_field :attraction_id, value: @attraction.id%>
    <%= f.submit "Go on this ride" %>
    <%end%>

<%= link_to "Edit Attraction", edit_attraction_path(@attraction)if current_user.admin %>